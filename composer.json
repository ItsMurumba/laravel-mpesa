{
    "name": "itsmurumba/laravel-mpesa",
    "description": "Laravel Package for Mpesa Daraja API",
    "license": "MIT",
    "keywords": [
        "mpesa",
        "laravel",
        "itsmurumba",
        "daraja"
    ],
    "autoload": {
        "psr-4": {
            "Itsmurumba\\Mpesa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itsmurumba\\Mpesa\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Kelvin Murumba",
            "email": "kevmurumba@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itsmurumba\\Mpesa\\MpesaServiceProvider"
            ]
        }
    }
}
